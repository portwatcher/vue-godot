{
  "name": "@vue-godot/runtime-tscn",
  "version": "0.0.1",
  "description": "Vue runtime renderer for Godot TSCN files",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npx tsc -p ."
  },
  "dependencies": {
    "@vue/runtime-core": "^3.3.0",
    "@vue/shared": "^3.3.0"
  },
  "peerDependencies": {
    "vue": "3.3.0"
  },
  "keywords": [
    "vue",
    "godot",
    "tscn",
    "renderer"
  ],
  "author": "Portwatcher",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^22.13.10",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2"
  }
}
